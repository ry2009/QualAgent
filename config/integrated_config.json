{
  "general": {
    "log_dir": "logs",
    "enable_file_logging": true,
    "enable_console_logging": true,
    "log_level": "INFO"
  },
  "android": {
    "android_task": "settings_wifi",
    "avd_name": "AndroidWorldAvd",
    "enable_screenshots": true,
    "screenshot_quality": 80
  },
  "agents": {
    "planner": {
      "model": "gemini-1.5-flash",
      "provider": "google",
      "api_key": "${GCP_API_KEY}",
      "temperature": 0.1,
      "max_tokens": 2000,
      "confidence_threshold": 0.7
    },
    "executor": {
      "model": "gemini-1.5-flash",
      "provider": "google",
      "api_key": "${GCP_API_KEY}",
      "temperature": 0.1,
      "max_tokens": 1500,
      "confidence_threshold": 0.7
    },
    "verifier": {
      "model": "gemini-1.5-flash",
      "provider": "google",
      "api_key": "${GCP_API_KEY}",
      "temperature": 0.1,
      "max_tokens": 1500,
      "confidence_threshold": 0.7
    },
    "supervisor": {
      "model": "gemini-1.5-flash",
      "provider": "google",
      "api_key": "${GCP_API_KEY}",
      "temperature": 0.2,
      "max_tokens": 2500,
      "confidence_threshold": 0.7
    }
  },
  "coordination": {
    "max_retries": 3,
    "enable_replanning": true,
    "enable_supervision": true,
    "continue_on_failure": true
  },
  "integration": {
    "framework": "Agent-S + QualGent",
    "use_agent_s_planning": true,
    "use_agent_s_execution": true,
    "use_qualgent_verification": true,
    "use_qualgent_supervision": true
  }
} 